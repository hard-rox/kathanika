# Environment Configuration Template
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# MongoDB Configuration
MONGO_PASSWORD=changeme

# Application Environment
ENVIRONMENT=Production

# Logging Level (Debug, Information, Warning, Error, Critical)
LOG_LEVEL=Information

# OpenTelemetry Endpoint (optional)
# For docker-compose: Use OTEL_ENDPOINT (mapped to OTEL_EXPORTER_OTLP_ENDPOINT inside container)
# For Kubernetes/Helm/direct deployments: Set OTEL_EXPORTER_OTLP_ENDPOINT directly
OTEL_ENDPOINT=
