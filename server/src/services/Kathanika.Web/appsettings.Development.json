      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://192.168.0.204:5341"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithThreadId",
      "WithExceptionDetails",
      "WithClientIp",
      "WithClientAgent"
    ],
    "Properties": {
      "Application": "Kathanika ILS"
    }
