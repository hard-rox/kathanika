{
  "name": "kathanika",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build kn-ui && ng build ils-web --configuration production --optimization=true",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:ils-web": "node dist/ils-web/server/server.mjs",
    "lint": "ng lint",
    "kn-ui:sb": "ng run kn-ui:storybook",
    "codegen": "graphql-codegen --config src/app-projects/ils-web/codegen.ts"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-server": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/service-worker": "^21.1.3",
    "@angular/ssr": "^21.0.4",
    "@apollo/client": "^4.1.4",
    "apollo-angular": "^13.0.0",
    "express": "^5.2.1",
    "graphql": "^16.12.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.26.17",
    "tslib": "^2.3.0",
    "tus-js-client": "^4.3.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2101.3",
    "@angular-devkit/build-angular": "^21.1.3",
    "@angular-devkit/core": "^21.1.3",
    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@compodoc/compodoc": "^1.2.1",
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/typescript": "^5.0.7",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.7",
    "@storybook/addon-a11y": "10.2.3",
    "@storybook/addon-docs": "10.2.3",
    "@storybook/addon-onboarding": "10.2.3",
    "@storybook/angular": "10.2.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/express": "^5.0.6",
    "@types/node": "^25.1.0",
    "@vitest/coverage-v8": "^4.0.18",
    "angular-eslint": "21.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "10.2.0",
    "jsdom": "^27.4.0",
    "ng-packagr": "^21.1.0",
    "postcss": "^8.5.3",
    "storybook": "10.2.3",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.54.0",
    "vitest": "^4.0.8"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
