{
  "name": "kathanika-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --browsers=ChromeHeadless",
    "test-ci": "ng test --code-coverage --no-watch --browsers=ChromeHeadless",
    "codegen": "graphql-codegen --config codegen.ts",
    "code-cov": "ng test --no-watch --code-coverage && http-server -p 4201 -c-1 coverage/kathanika-app -o"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.2",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.2",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.2",
    "@angular/platform-browser": "^16.1.2",
    "@angular/platform-browser-dynamic": "^16.1.2",
    "@angular/router": "^16.1.0",
    "@angular/service-worker": "^16.1.2",
    "@apollo/client": "^3.7.16",
    "apollo-angular": "^5.0.0",
    "graphql": "^16",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.7.12",
    "tslib": "^2.5.3",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular/cli": "~16.1.1",
    "@angular/compiler-cli": "^16.1.2",
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-apollo-angular": "^3.5.6",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/jasmine": "~4.3.4",
    "autoprefixer": "^10.4.14",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.0.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.1.3"
  }
}
